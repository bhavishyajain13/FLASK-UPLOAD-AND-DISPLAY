import asyncio

async def async_entry_point(f_nm_temp):
    zzz = []

    async def async_fun1():
        nonlocal zzz
        print("in func1")
        result_az = get_ans("temp_img_store/crop_img1.jpg")
        print("func1 az done")
        result_gpt = gpt_format(result_az, prompt1)
        print("func1 gpt done")

        f_ans = comp_validation_1(result_gpt, result_az)
        for temp in f_ans:
            print(temp)
            zzz.append(str(temp))

    async def async_fun2():
        nonlocal zzz
        print("in func2")
        result_az2 = get_ans("temp_img_store/crop_img2.jpg")
        print(" func2 az done")
        result_gpt2 = gpt_format(result_az2, prompt2)
        print("func2 gpt done")

        f_ans2 = comp_validation_2(result_gpt2)
        for temp in f_ans2:
            print(str(temp))
            zzz.append(str(temp))

    await asyncio.gather(async_fun1(), async_fun2())
    print(str(zzz))

asyncio.run(async_entry_point("form2_test.JPG"))
